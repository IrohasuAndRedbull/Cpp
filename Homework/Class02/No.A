#include <stdio.h>

void swap(int* num) {
    *num = 0;
}

int main() {
    int i = 0, a[5] = {};
    for (i = 0; i < 5; i++) {
        printf("Input a number for a[%d] : ", i); scanf_s("%d", &a[i]);
    }

    printf("-----------------------------\n");

    for (i = 0; i < 5; i++) {
        swap(&a[i]);
        printf("Updated number for a[%d] is %d\n", i, a[i]);
    }

    return 0;
}
